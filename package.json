{
  "name": "replit-desktop",
  "productName": "Replit Desktop",
  "version": "2.0",
  "description": "Desktop version of replit",
  "keywords": [
    "electron",
    "repl.it",
    "Replit",
    "Replit Desktop",
    "Program editor",
    "discord-rpc"
  ],
  "repository": "https://github.com/replit-discord/replit-desktop",
  "license": "Apache-2.0",
  "author": "leon332157&repl-it-discord",
  "main": "main.js",
  "scripts": {
    "build:app:test": "electron-builder -c electron-builder.conf.js --dir",
    "build:dev": "gulp buildDev",
    "build:prod": "gulp buildProd",
    "build:run": "electron --trace-warnings -no-sandbox ./ts-out/",
    "dist": "gulp buildApp",
    "pre-dist": "gulp buildAppPreRelease",
    "start": "gulp buildAndRun",
    "watch": "gulp watchDev",
    "lint:check": "eslint \"**/*.{js,ts,json}\"",
    "lint": "eslint \"**/*.{js,ts,json}\" --fix"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "discord-rpc": "==4.0.1",
    "electron-context-menu": "^2.5.1",
    "semver": "^7.3.8",
    "write-file-atomic": "^3"
  },
  "devDependencies": {
    "@octokit/types": "^6.13.0",
    "@sucrase/gulp-plugin": "^2.0.0",
    "@types/discord-rpc": "^3.0.6",
    "@types/gulp": "^4.0.9",
    "@types/node": "^18",
    "@types/semver": "^7.3.8",
    "@types/write-file-atomic": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "electron": "^13",
    "electron-builder": "^22.11.11",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^3.4.1",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-json-editor": "^2.5.6",
    "gulp-terser-js": "^5.2.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "prettier": "^2.3.2",
    "sucrase": "^3.20.1",
    "typescript": "^4.3.5"
  },
  "email": "leon331257@gmail.com"
}
